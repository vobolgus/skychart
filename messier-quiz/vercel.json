{
  "buildCommand": "npm run build",
  "outputDirectory": "dist",
  "framework": null,
  "rewrites": [
    { "source": "/", "destination": "/src/pages/index.html" },
    { "source": "/game", "destination": "/src/pages/game.html" },
    { "source": "/debug", "destination": "/src/pages/debug.html" },
    { "source": "/src/(.*)", "destination": "/src/$1" },
    { "source": "/public/(.*)", "destination": "/public/$1" }
  ]
}